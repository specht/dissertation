issn = {1471-2105},
journal = {BMC Bioinformatics},
number = {1},
pages = {15},
shorttitle = {AYUMS},
title = {{AYUMS: an algorithm for completely automatic quantitation based on LC-MS/MS proteome data and its application to the analysis of signal transduction}},
