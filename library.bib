issn = {1367-4803},
journal = {Bioinformatics},
keywords = {Algorithms,Computer Graphics,Database Management Systems,Databases,Information Storage and Retrieval,Information Storage and Retrieval: methods,Peptide Mapping,Peptide Mapping: methods,Programming Languages,Protein,Proteome,Proteome: chemistry,Proteomics,Proteomics: methods,Software,User-Computer Interface},
month = jan,
