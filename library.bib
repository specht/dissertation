title = {{Comparative Evaluation of Tandem MS Search Algorithms Using a Target-Decoy Search Strategy}},
url = {http://www.mcponline.org/content/6/9/1599.abstract http://www.mcponline.org/content/6/9/1599.full},
volume = {6},
year = {2007}
}
