pages = {393--403},
title = {{MSQuant, an Open Source Platform for Mass Spectrometry-Based Quantitative Proteomics}},
url = {http://dx.doi.org/10.1021/pr900721e http://pubs.acs.org/doi/full/10.1021/pr900721e},
volume = {9},
year = {2010}
}
