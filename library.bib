issn = {1615-9853},
journal = {Proteomics},
keywords = {Algorithms,Benchmarking,Blood Proteins,Blood Proteins: chemistry,Computational Biology,Databases, Protein,False Positive Reactions,Humans,Internet,Mass Spectrometry,Mass Spectrometry: methods,Peptides,Peptides: chemistry,Proteomics,Proteomics: methods,ROC Curve,Reference Standards,Sensitivity and Specificity,Software,Trypsin,Trypsin: pharmacology},
month = aug,
number = {13},
