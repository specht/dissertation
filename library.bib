mendeley-tags = {Amino Acid Sequence,Animals,Base Sequence,Computational Biology,Expressed Sequence Tags,Genes,Genome- Human,Genomics,Humans,Mice,Sequence Alignment,Sequence Analysis- DNA,Sequence Analysis- Protein,Software},
pages = {S11.1--8},
shorttitle = {AUGUSTUS at EGASP},
title = {{AUGUSTUS at EGASP: using EST, protein and genomic alignments for improved gene prediction in the human genome}},
