journal = {Bioinformatics},
month = jan,
number = {2},
pages = {e191--197},
title = {{TOPP -- the OpenMS proteomics pipeline}},
url = {http://bioinformatics.oxfordjournals.org/cgi/content/abstract/23/2/e191},
volume = {23},
year = {2007}
}
