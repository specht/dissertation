keywords = {Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: analysis,Arabidopsis: chemistry,Databases, Protein,Mass Spectrometry,Mass Spectrometry: methods,Nitrogen Isotopes,Peptides,Peptides: analysis,Proteomics,Proteomics: methods,Software},
month = oct,
number = {19},
pages = {3462--9},
pmid = {17726679},
title = {{Quantitative proteomics using uniform (15)N-labeling, MASCOT, and the trans-proteomic pipeline.}},
