issn = {1088-9051},
journal = {Genome Research},
keywords = {Algorithms,Alternative Splicing,Amino Acid Sequence,Databases,Exons,Genetic Techniques,Humans,Introns,Mass Spectrometry,Mass Spectrometry: methods,Molecular Sequence Data,Peptides,Peptides: chemistry,Peptides: genetics,Polymorphism,Protein,Protein Array Analysis,Protein Array Analysis: methods,Protein Array Analysis: statistics \& numerical dat,Proteomics,Proteomics: methods,Proteomics: statistics \& numerical data,Sequence Alignment,Single Nucleotide},
month = feb,
