mendeley-tags = {Alternative Splicing,Animals,Computational Biology,Databases- Genetic,Genes,Genome- Human,Genomics,Humans,Mice,RNA- Messenger,Sequence Analysis- DNA,Sequence Analysis- RNA},
month = jan,
pages = {S2.1--31},
pmid = {16925836},
